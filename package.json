{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio --host 127.0.0.1",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "postinstall": "drizzle-kit generate",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.5.7",
    "@builder.io/qwik-city": "^1.5.7",
    "@qwik-ui/headless": "^0.4.4",
    "@qwik-ui/styled": "^0.1.0",
    "@qwik-ui/utils": "^0.2.1",
    "@qwikest/icons": "^0.0.13",
    "@types/compression": "^1.7.5",
    "@types/eslint": "^8.56.10",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "autoprefixer": "^10.4.19",
    "compression": "^1.7.4",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.22.7",
    "eslint": "^8.57.0",
    "eslint-plugin-qwik": "^1.5.7",
    "express": "4.19.2",
    "nx": "^19.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "qwik-lucia": "^0.5.1",
    "qwik-ui": "^0.1.0",
    "tailwindcss": "3.4.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.4.5",
    "undici": "^6.19.0",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "nx": {},
  "dependencies": {
    "@libsql/client": "^0.6.2",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "drizzle-orm": "^0.31.2",
    "lucia": "^3.2.0",
    "pg": "^8.12.0"
  }
}
